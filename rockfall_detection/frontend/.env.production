# React Frontend Environment Configuration - PRODUCTION (Frontend Only)
# ========================================================================

# Application Settings
VITE_APP_TITLE=Rockfall Detection System
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production
VITE_MODE=standalone

# Feature Flags - Production Ready
VITE_ENABLE_LIVE_MONITORING=true
VITE_ENABLE_DEM_ANALYSIS=true
VITE_ENABLE_RISK_ASSESSMENT=true
VITE_ENABLE_DETECTION=true
VITE_ENABLE_DEBUG_MODE=false

# UI Configuration - Production Optimized
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=true
VITE_AUTO_REFRESH_INTERVAL=10000

# Mock Data Configuration - Production Demo
VITE_ENABLE_MOCK_DATA=true
VITE_USE_STATIC_DATA=true
VITE_SIMULATE_REAL_TIME=true

# Notification Settings
VITE_NOTIFICATION_ENABLED=true
VITE_NOTIFICATION_SOUND=false
VITE_HIGH_RISK_THRESHOLD=75
VITE_NOTIFICATION_COOLDOWN=300000

# Camera Configuration - Cloudinary URLs (Production)
VITE_CAMERA_ENABLED=true
VITE_CAMERA_REFRESH_RATE=24
VITE_CAMERA_EAST_URL=https://res.cloudinary.com/dyb6aumhm/video/upload/v1758167914/1_znxt5x.mp4
VITE_CAMERA_WEST_URL=https://res.cloudinary.com/dyb6aumhm/video/upload/v1758167915/2_lrgtxq.mp4
VITE_CAMERA_NORTH_URL=https://res.cloudinary.com/dyb6aumhm/video/upload/v1758167915/3_gk37sc.mp4

# Risk Assessment Settings - Production
VITE_RISK_UPDATE_INTERVAL=10000
VITE_RISK_FORCE_HIGH_CHANCE=0.1
VITE_RISK_CALCULATION_MODE=enhanced

# Detection Settings
VITE_DETECTION_CONFIDENCE_THRESHOLD=0.8
VITE_DETECTION_DEMO_MODE=true

# Performance Settings - Production Optimized
VITE_ENABLE_CACHE=true
VITE_CACHE_DURATION=300000
VITE_ENABLE_COMPRESSION=true
VITE_DEBUG_PERFORMANCE=false

# Error Handling - Production
VITE_SHOW_ERROR_DETAILS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_ERROR_BOUNDARY=true
VITE_FALLBACK_MODE=true

# Analytics & Monitoring (Optional)
VITE_GOOGLE_ANALYTICS_ID=
VITE_ENABLE_ANALYTICS=false
VITE_SENTRY_DSN=
VITE_ENABLE_ERROR_REPORTING=false